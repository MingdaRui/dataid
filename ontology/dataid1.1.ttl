#    Datatypes
#
#################################################################


###  http://dataid.dbpedia.org/ns/core#md5

datid:md5 rdf:type rdfs:Datatype ;
          
          rdfs:comment "A md5 checksum" ;

		  rdfs:label "md5 checksum" ; 
		  owl:onDatatype xsd:string ;
	      owl:withRestrictions (
		    [
			  xsd:pattern "[a-z0-9]{32}"
		    ]
	      ) .





